# AgroAI Backend Development Environment Configuration
# Copy this file to .env.development and update the values

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=agroai_user
DB_PASSWORD=agroai_password
DB_NAME=agroai_dev
DB_SSL_MODE=disable

# Test Database
TEST_DB_NAME=agroai_test

# API Configuration
API_PORT=8080
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
CORS_DEBUG=true

# JWT Configuration
JWT_SECRET=agroai_dev_jwt_secret_key_2024
JWT_EXPIRES_IN=24h

# Redis Configuration (Optional)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=agroai_redis_password

# Payment Providers (Development Keys)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
MPESA_CONSUMER_KEY=your_mpesa_consumer_key
MPESA_CONSUMER_SECRET=your_mpesa_consumer_secret
PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_CLIENT_SECRET=your_paypal_client_secret

# Email Configuration (Development)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password

# File Upload Configuration
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=10485760

# Logging
LOG_LEVEL=debug
LOG_FORMAT=json
